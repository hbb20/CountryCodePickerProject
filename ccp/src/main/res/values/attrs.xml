<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountryCodePicker">
        <!--{@deprecated Use defaultNameCode instead.}-->
        <attr name="defaultCode" format="integer"/>
        <attr name="contentColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="arrowSize" format="dimension"/>
        <attr name="countryPreference" format="string"/>
        <attr name="defaultNameCode" format="string"/>
        <attr name="hideNameCode" format="boolean"/>
        <!--Make sure: order in this list must match order of Language Enum in CountryCodePicker.java. Value must be ascending and starting from 0.-->
        <attr name="ccpLanguage" format="enum">
            <enum name="ARABIC" value="0"/>
            <enum name="BENGALI" value="1"/>
            <enum name="CHINESE_SIMPLIFIED" value="2"/>
            <enum name="ENGLISH" value="3"/>
            <enum name="FRENCH" value="4"/>
            <enum name="GERMAN" value="5"/>
            <enum name="GUJARATI" value="6"/>
            <enum name="HINDI" value="7"/>
            <enum name="JAPANESE" value="8"/>
            <enum name="INDONESIA" value="9"/>
            <enum name="PORTUGUESE" value="10"/>
            <enum name="RUSSIAN" value="11"/>
            <enum name="SPANISH" value="12"/>
            <enum name="HEBREW" value="13"/>
            <enum name="CHINESE_TRADITIONAL" value="14"/>
            <enum name="KOREAN" value="15"/>
        </attr>
        <attr name="keyboardAutoPopOnSearch" format="boolean"/>
        <attr name="customMasterCountries" format="string"/>
        <attr name="showFlag" format="boolean"/>
        <attr name="showFullName" format="boolean"/>
        <attr name="selectionDialogShowSearch" format="boolean"/>
        <attr name="ccpClickable" format="boolean"/>
        <attr name="ccp_showPhoneCode" format="boolean"/>
        <attr name="ccpSearchBox_showPhoneCode" format="boolean"/>
        <attr name="ccpDialog_showFastScroller" format="boolean"/>
        <attr name="ccpDialog_backgroundColor" format="color"/>
        <attr name="ccpDialog_textColor" format="color"/>
        <attr name="ccpDialog_fastScroller_bubbleColor" format="color"/>
        <attr name="ccpDialog_fastScroller_handleColor" format="color"/>
        <attr name="ccpDialog_fastScroller_bubbleTextAppearance" format="reference"/>
        <attr name="ccp_flagBorderColor" format="color"/>
    </declare-styleable>
</resources>